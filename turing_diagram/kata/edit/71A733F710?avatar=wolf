#include &quot;untitled.h&quot;
#include &lt;assert.h&gt;
#include &lt;stdio.h&gt;

static void example(void)
{
    assert(hhg() == 6*9);
}

typedef void test(void);

static test * tests[ ] =
{
    example,
    (test*)0,
};

int main(void)
{
    size_t at = 0;
    while (tests[at])
    {
        tests[at++]();
        putchar('.');
    }
    printf(&quot;\n%zd tests passed&quot;, at);
    return 0;
}

</textarea>                                     
